            android:theme="@style/SplashscreenTheme" />
        <activity android:name="com.asiantech.intern20summer1.week4.activity.SignInActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".week5.activity.TimeLineActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>
